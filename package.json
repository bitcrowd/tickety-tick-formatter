{
  "name": "tickety-tick-formatter",
  "version": "1.0.0",
  "description": "Format your tickets into git-digestable branch names and commit messages",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "del-cli dist && tsc --project .",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:json": "prettier --ignore-path .gitignore --check '**/*.json'",
    "lint:md": "prettier --ignore-path .gitignore --check '**/*.md'",
    "lint": "run-s --continue-on-error lint:js lint:json lint:md",
    "test": "jest",
    "checks": "run-s lint test"
  },
  "keywords": [
    "git",
    "tickety-tick",
    "formatters",
    "nodejs",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcrowd/tickety-tick-formatter.git"
  },
  "author": "bitcrowd <tickety-tick@bitcrowd.net>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^10.12.10",
    "@types/prettier": "^2.2.1",
    "@types/speakingurl": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "prettier": "^2.2.1",
    "speakingurl": "^14.0.1",
    "strip-indent": "^3.0.0"
  }
}
